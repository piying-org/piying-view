{
  "name": "@piying/view-vue2-legacy",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -p tsconfig.app.json && vite build && vue-tsc -p tsconfig.type.json",
    "preview": "vite preview",
    "test": "vitest --config ./vitest.config.ts",
    "lint": "eslint ./src --fix && prettier ./src --write",
    "local-publish": "npm run build && npm --registry=http://lan-server.chloc:5555 publish ./dist",
    "remote-publish": "npm run build && npm publish --access=public --registry=https://registry.npmjs.org ./dist"
  },
  "dependencies": {
    "vue": "2.7.16"
  },
  "devDependencies": {
    "@vitejs/plugin-vue2": "2.3.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "1.3.6",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-vue": "~10.0.0",
    "vue-tsc": "^3.2.2",
    "vitest": "4.0.16",
    "vite": "7.3.1",
    "@vitest/browser-playwright": "^4.0.16"
  }
}